{
  "name": "아스 - Ajou Swift",
  "version": "1.2.4",
  "description": "아주대학교 재학생을 위한 교내 페이지 모음 확장 프로그램",
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "https://fonts.googleapis.com/earlyaccess/notosanskr.css"
  ],
  "web_accessible_resources": [{
    "resources": [
      "data/page.json", 
      "data/schedule.json"
    ],
    "matches": [
      "file:///*"
    ]
  }],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": {
      "16": "images/logo/Ajou_logo16.png",
      "32": "images/logo/Ajou_logo32.png",
      "48": "images/logo/Ajou_logo48.png",
      "128": "images/logo/Ajou_logo128.png"
    }
  },
  "options_page": "options/options.html",
  "icons": {
    "16": "images/logo/Ajou_logo16.png",
    "32": "images/logo/Ajou_logo32.png",
    "48": "images/logo/Ajou_logo48.png",
    "128": "images/logo/Ajou_logo128.png"
  },
  "manifest_version": 3
}